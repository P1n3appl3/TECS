/**
 * Takes a shortcut
 */

CHIP Or16Way {
    IN in[16];
    OUT out; 

    PARTS:
	Or8Way(in=in[0..7],out=w1);
	Or8Way(in=in[8..15],out=w2);
	Or(a=w1,b=w2,out=out);
}
