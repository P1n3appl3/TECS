class Util {

    function int mod(int a, int b){
        return a - ((a/b) * b);
    }

    function int abs(int a){
        if(a<0){
            return -a;
        }
        return a;
    }

    function int rand(){
        return 1;
    }

    function void dispose2DArray(Array arr, int size){
        var int i, temp;
        let i=0;
        while(i<size){
            let temp = arr[i];
            do Array.dispose(temp);
        }
        return;
    }
}
